{
  "db": {
    "dsn": {
      "host": "localhost",
      "port": 5432,
      "database": "logicsem",
      "user": "logicsem",
      "password": "XXX",
      "application_name": "dpdb"
    },
    "max_connections": 100
  },
  "db_admin": {
    "host": "localhost",
    "port": 5432,
    "database": "logicsem",
    "user": "postgres",
    "password": "XXX",
    "application_name": "dpdb-admin"
  },
  "htd": {
    "path": "../htd/bin/htd_main",
    "parameters": [
      "--child-limit",
      "5"
    ]
  },
  "dpdb": {
  },
  "problem_specific": {
    "nestpmc": {
      "max_solver_threads": 12,
      "inner_vars_threshold": 40,
      "max_worker_threads": 12
    },
    "pmcext": {
      "preprocessor": {
        "path": "../pmc_linux",
        "args": "-vivification -eliminateLit -litImplied -iterate=10 -equiv -orGate -affine"
      },
      "sat_solver": {
        "path": "../picosat",
        "seed_arg": "-s"
      },
      "max_solver_threads": 12
    }
  },
  "nesthdb": {
    "threshold_hybrid": 45,
    "threshold_abstract": 8,
    "max_recursion_depth": 1,
    "sat_solver": {
      "path": "<PATH-TO-PICOSAT>/picosat-965",
      "seed_arg": "-s"
    },
    "sharpsat_solver": {
      "path": "<PATH-TO-miniC2D>/miniC2D-1.0.0",
      "args": "-C -c",
      "output_parser": {
        "class": "RegExReader",
        "args": {
          "pattern": "Counting... (\\d+) models"
        },
        "result": "result"
      }
    },
    "pmc_solver": {
      "path": "<PATH-TO-PROJCLINGO>>/projClingo-wrapper-1.0",
      "output_parser": {
        "class": "RegExReader",
        "args": {
          "pattern": "Models.*: (\\d+)\\n"
        },
        "result": "result"
      }
    },
    "preprocessor": {
      "path": "<PATH-TO-PMC>/pmc-1.0",
      "args": "-vivification -eliminateLit -litImplied -iterate=10"
    },
    "asp": {
      "encodings": [
        {
          "file": "guess_min_degree.lp",
          "size": 95,
          "timeout": 10
        }
      ]
    }
  }
}
